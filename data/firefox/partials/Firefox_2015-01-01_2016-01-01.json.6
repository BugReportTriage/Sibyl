  "component" : "Security",
  "summary" : "Site identity data is not updated in some cases of weird certificates",
  "reporter" : "mh+mozilla@glandium.org",
  "assigned" : "gijskruitbosch+bugs@gmail.com",
  "created" : null,
  "operatingSystem" : "All",

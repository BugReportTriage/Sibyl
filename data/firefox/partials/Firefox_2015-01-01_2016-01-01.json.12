  "component" : "Developer Tools: Inspector",
  "summary" : "Add an 'Add rule' toolbar button in the rules view",
  "reporter" : "ntim.bugs@gmail.com",
  "assigned" : "ntim.bugs@gmail.com",
  "created" : null,
  "operatingSystem" : "All",
  "hardware" : "All",

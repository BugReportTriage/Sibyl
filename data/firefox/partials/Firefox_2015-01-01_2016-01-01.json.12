  "summary" : "[e10s] \"Copy Email Address\" in remote browser causes unsafe CPOW usage warning",
  "reporter" : "moz-ian@perix.co.uk",
  "assigned" : "moz-ian@perix.co.uk",
  "created" : null,
  "operatingSystem" : "All",

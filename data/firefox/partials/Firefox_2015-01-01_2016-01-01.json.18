  "component" : "PDF Viewer",
  "summary" : "Update pdf.js to version 1.1.24",
  "reporter" : "ryanvm@gmail.com",
  "assigned" : "ryanvm@gmail.com",
  "created" : null,
  "operatingSystem" : "All",

  "summary" : "Performance timeline legends should be dark with light text in dark theme in LineGraphs",
  "reporter" : "jordan@jsantell.com",
  "assigned" : "vporof@mozilla.com",
  "created" : null,
  "operatingSystem" : "Mac OS X",
  "hardware" : "x86",

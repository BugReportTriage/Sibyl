  "component" : "General",
  "summary" : "The focusring for the controls in reader mode doesn't match the visible borders of the buttons",
  "reporter" : "andrei.vaida@softvision.ro",
  "assigned" : "jaws@mozilla.com",
  "created" : null,
  "operatingSystem" : "All",
  "hardware" : "All",
